{
  "name": "dvc",
  "private": true,
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "iterative",
  "icon": "docs/dvc.png",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "readme": "./README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/iterative/vscode-dvc"
  },
  "bugs": "https://github.com/iterative/vscode-dvc",
  "homepage": "https://github.com/iterative/vscode-dvc/README.md",
  "keywords": [
    "iterative",
    "data",
    "version",
    "control",
    "dvc"
  ],
  "categories": [
    "Data Science",
    "Visualization"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.53.0",
    "node": ">=10.2.0"
  },
  "activationEvents": [
    "*",
    "onWebviewPanel:dvc-view"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "%command.add%",
        "command": "dvc.add",
        "category": "DVC"
      },
      {
        "title": "%command.checkout%",
        "command": "dvc.checkout",
        "category": "DVC"
      },
      {
        "title": "%command.checkoutRecursive%",
        "category": "DVC",
        "command": "dvc.checkoutRecursive"
      },
      {
        "title": "%command.commit%",
        "command": "dvc.commit",
        "category": "DVC"
      },
      {
        "title": "%command.fetch%",
        "command": "dvc.fetch",
        "category": "DVC"
      },
      {
        "title": "%command.fetchAllBranches%",
        "command": "dvc.fetchAllBranches",
        "category": "DVC"
      },
      {
        "title": "%command.gc%",
        "command": "dvc.gc",
        "category": "DVC"
      },
      {
        "title": "%command.init%",
        "command": "dvc.init",
        "category": "DVC"
      },
      {
        "title": "%command.initNoScm%",
        "command": "dvc.initNoScm",
        "category": "DVC"
      },
      {
        "title": "%command.install%",
        "command": "dvc.install",
        "category": "DVC"
      },
      {
        "title": "%command.list%",
        "command": "dvc.list",
        "category": "DVC"
      },
      {
        "title": "%command.pull%",
        "command": "dvc.pull",
        "category": "DVC"
      },
      {
        "title": "%command.push%",
        "command": "dvc.push",
        "category": "DVC",
        "icon": "media/push.svg"
      },
      {
        "title": "%command.destroy%",
        "command": "dvc.destroy",
        "category": "DVC"
      },
      {
        "title": "%command.status%",
        "command": "dvc.status",
        "category": "DVC"
      },
      {
        "title": "%command.showExperiments%",
        "command": "dvc.showExperiments",
        "category": "DVC"
      },
      {
        "title": "%command.runExperiment%",
        "command": "dvc.runExperiment",
        "category": "DVC"
      },
      {
        "title": "Select DVC path",
        "command": "dvc.selectDvcPath",
        "category": "DVC"
      },
      {
        "command": "explorerTreeView.openFile",
        "title": "Open File"
      },
      {
        "command": "explorerTreeView.refreshEntry",
        "title": "Refresh",
        "icon": "media/refresh.svg"
      }
    ],
    "configuration": {
      "title": "DVC",
      "properties": {
        "dvc.dvcPath": {
          "type": "string",
          "description": "Path to DVC.",
          "default": "dvc",
          "scope": "resource"
        }
      }
    },
    "menus": {
      "scm/title": [
        {
          "command": "dvc.showExperiments",
          "group": "1_header@1",
          "when": "scmProvider == dvc"
        },
        {
          "command": "dvc.runExperiment",
          "group": "1_header@2",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.init",
          "group": "2_main@1",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.checkout",
          "group": "2_main@2",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.commit",
          "group": "2_main@3",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.fetch",
          "group": "2_main@4",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.pushpull",
          "group": "2_main@5",
          "when": "scmProvider == dvc"
        },
        {
          "submenu": "dvc.dvcExtra",
          "group": "3_main@1",
          "when": "scmProvider == dvc"
        }
      ],
      "dvc.init": [
        {
          "command": "dvc.init",
          "group": "1_init@1"
        },
        {
          "command": "dvc.initNoScm",
          "group": "1_init@2"
        }
      ],
      "dvc.checkout": [
        {
          "command": "dvc.checkout",
          "group": "1_checkout@1"
        },
        {
          "command": "dvc.checkoutRecursive",
          "group": "1_checkout@2"
        }
      ],
      "dvc.commit": [
        {
          "command": "dvc.commit",
          "group": "1_commit@1"
        }
      ],
      "dvc.dvcExtra": [
        {
          "command": "dvc.install",
          "group": "1_extra@1"
        },
        {
          "command": "dvc.gc",
          "group": "1_extra@2"
        },
        {
          "command": "dvc.destroy",
          "group": "1_extra@3"
        },
        {
          "command": "dvc.list",
          "group": "1_extra@4"
        },
        {
          "command": "dvc.status",
          "group": "1_extra@5"
        }
      ],
      "dvc.fetch": [
        {
          "command": "dvc.fetch",
          "group": "1_fetch@1"
        },
        {
          "command": "dvc.fetchAllBranches",
          "group": "1_fetch@2"
        }
      ],
      "dvc.pushpull": [
        {
          "command": "dvc.push",
          "group": "1_push@1"
        },
        {
          "command": "dvc.pull",
          "group": "2_pull@1"
        }
      ],
      "scm/resourceState/context": [
        {
          "command": "dvc.push",
          "group": "inline"
        }
      ],
      "explorer/context": [
        {
          "command": "dvc.runExperiment",
          "when": "resourceFilename == params.yaml || resourceFilename == dvc.yaml",
          "group": "8_dvc@1"
        },
        {
          "command": "dvc.init",
          "when": "explorerResourceIsFolder",
          "group": "8_dvc@1"
        },
        {
          "command": "dvc.add",
          "when": "isFileSystemResource",
          "group": "8_dvc@2"
        },
        {
          "command": "dvc.checkout",
          "when": "explorerResourceIsFolder",
          "group": "8_dvc@3"
        },
        {
          "command": "dvc.checkoutRecursive",
          "when": "explorerResourceIsFolder",
          "group": "8_dvc@4"
        }
      ]
    },
    "submenus": [
      {
        "id": "dvc.checkout",
        "label": "%submenu.checkout%"
      },
      {
        "id": "dvc.commit",
        "label": "%submenu.commit%"
      },
      {
        "id": "dvc.dvcExtra",
        "label": "%submenu.dvcExtra%"
      },
      {
        "id": "dvc.fetch",
        "label": "%submenu.fetch%"
      },
      {
        "id": "dvc.init",
        "label": "%submenu.init%"
      },
      {
        "id": "dvc.pushpull",
        "label": "%submenu.pushpull%"
      }
    ]
  },
  "enableProposedApi": true,
  "scripts": {
    "dev": "shx rm -rf dist && tsc -watch -p ./",
    "build": "yarn build-webpack && yarn package",
    "build-webpack": "webpack --mode production",
    "package": "vsce package --yarn",
    "pub": "vsce publish --yarn --baseImagesUrl https://github.com/iterative/vscode-dvc/raw/master/extension --baseContentUrl https://github.com/iterative/vscode-dvc/raw/master/extension",
    "format": "prettier --write '**/*.{js,ts,md,json}'",
    "lint": "eslint --cache --fix '**/*.{js,ts}'",
    "build-vscode-tests": "tsc -p './'",
    "run-vscode-tests": "node ./dist/test/runTest.js",
    "test:vscode": "run-s build-vscode-tests run-vscode-tests",
    "test:jest": "jest",
    "test": "run-p test:*",
    "test-staged": "lint-staged",
    "vscode:prepublish": ""
  },
  "dependencies": {
    "@hediet/node-reload": "^0.7.3",
    "@hediet/std": "^0.6.0",
    "bufferutil": "^4.0.3",
    "chokidar": "^3.5.1",
    "dayjs": "^1.10.1",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "lodash.debounce": "^4.0.8",
    "mobx": "^6.0.4",
    "utf-8-validate": "^5.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.19",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/sinon-chai": "^3.2.5",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "vsce": "^1.85.0",
    "vscode-test": "^1.4.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "jest --bail --findRelatedTests"
  }
}
