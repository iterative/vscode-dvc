  "mochaExplorer.require": ["ts-node/register", "source-map-support/register"],
  "tasksStatusbar.taskLabelFilter": "dev",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "DVCPATH",
    "MNIST",
    "symlinked",
    "toplevel",
    "venv",
    "virtualenv"
  ]
