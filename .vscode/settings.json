{
  "search.exclude": {
    "extension/out": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "plantuml.diagramsRoot": "docs",
  "plantuml.exportFormat": "png",
  "plantuml.exportOutDir": "docs/exported",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
