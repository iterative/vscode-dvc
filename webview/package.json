    "jest": "29.5.0",
    "jest-canvas-mock": "2.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.1",
    "raw-loader": "4.0.2",
    "sass": "1.61.0",
    "sass-loader": "13.2.2",
    "storybook-addon-designs": "6.3.1",
