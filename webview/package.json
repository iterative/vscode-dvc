    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@svgr/cli": "7.0.0",
    "@swc/core": "1.3.49",
    "@swc/jest": "0.2.24",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.0",
    "@types/jsdom": "21.1.1",
    "@types/node": "16.x",
