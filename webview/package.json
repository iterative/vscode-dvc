    "@vscode/webview-ui-toolkit": "1.2.2",
    "classnames": "2.3.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.merge": "4.6.2",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.3",
    "react-redux": "8.0.5",
    "react-vega": "7.6.0",
